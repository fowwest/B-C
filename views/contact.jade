  // ================Get in Touch Area =================
  section.get_in_touch_area.p_100
    .container
      .row.get_touch_inner
        .col-lg-6
          form#contactForm.contact_us_form.row(action='contact_process.php', method='post', novalidate='novalidate')
            .form-group.col-lg-6
              input#name.form-control(type='text', name='name', placeholder='Name')
            .form-group.col-lg-6
              input#email.form-control(type='email', name='email', placeholder='Email')
            .form-group.col-lg-12
              input#subject.form-control(type='text', name='subject', placeholder='Subject*')
            .form-group.col-lg-12
              textarea#message.form-control(name='message', rows='1', placeholder='Message')
            .form-group.col-lg-12
              button.btn.submit_btn.form-control(type='submit', value='submit') Send
        .col-lg-6
          .touch_details
            .l_title
              img(src='img/icon/title-icon.png', alt='')
